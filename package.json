{
  "name": "postronaut-frontend",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/api": {
      "target": "https://postronaut-backend.herokuapp.com/",
      "ws": true
    }
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "starts": "node server.js",
    "postinstall": "npm run build",
    "start": "npm run serve"
  },
  "dependencies": {
    "aws-sdk": "^2.1046.0",
    "axios": "^0.24.0",
    "core-js": "^3.6.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.2",
    "json-form-data": "^1.7.2",
    "moment": "^2.29.1",
    "portal-vue": "^2.1.7",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-vue": "^0.11.0",
    "serve-static": "^1.14.2",
    "uuid": "^3.4.0",
    "vue": "^2.6.11",
    "vue-infinite-loading": "^2.4.5",
    "vue-router": "^3.5.3",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.0",
    "@vue/cli-service": "^4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "error"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
